{
  "name": "Crystal Language API Redirect",
  "description": "Redirect all Crystal API references to the latest version",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["src/background.js"]
  },
  "permissions": [
    "webRequest",
    "*://crystal-lang.org/api",
    "*://www.crystal-lang.org/api",
    "webRequestBlocking"
  ]
}
